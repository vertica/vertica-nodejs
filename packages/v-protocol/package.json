{
  "name": "v-protocol",
  "version": "1.1.0",
  "description": "The Vertica client/server binary protocol, implemented in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "chai": "^4.2.0",
    "chunky": "^0.0.0",
    "mocha": "^7.1.2",
    "ts-node": "^8.5.4",
    "typescript": "^4.0.3"
